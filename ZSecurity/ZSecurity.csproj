<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Title>ZSecurity</Title>
		<Authors>Ricardo Zambon</Authors>
		<Company>ZambonSoft</Company>
		<Version>1.0.0</Version>
		<RepositoryUrl>https://github.com/RicardoZambon/ZSecurity</RepositoryUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<Description>ZSecurity is a library that brings additional functionalities for handling security.</Description>
		<Copyright>ZambonSoft</Copyright>
		<PackageProjectUrl>https://github.com/RicardoZambon/ZSecurity</PackageProjectUrl>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="ZDatabase" Version="1.0.0" />
	</ItemGroup>

</Project>
